{
  "compilerOptions": {
    "jsx": "react-jsx", // Para React 17 o posterior
    // "jsx": "react", // Para versiones anteriores de React
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
